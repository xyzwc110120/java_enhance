<?com.cyx.java_enhance.xml version="1.0" encoding="UTF-8" ?>
<!-- 引用 dtd 约束，students 为根元素名称，因为在同一文件夹下，所以直接用文件名引用 -->
<!DOCTYPE students SYSTEM "studentsDtd.dtd">

<students>
    <student>
        <name>张三</name>
        <age>19</age>
        <sex>男</sex>
    </student>
    <student>
        <name>如花</name>
        <age>20</age>
        <sex>女</sex>
    </student>

    <!-- 元素的顺序不能颠倒 -->
    <!--
        <student>
            <name>王五</name>
            <sex>男</sex>
            <age>19</age>
        </student>
    -->
</students>